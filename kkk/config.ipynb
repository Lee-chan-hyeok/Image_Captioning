    "import json\n",
    "\n",
    "class Config:\n",
    "    def __init__(self, path):\n",
    "        self.path = path\n",
    "        with open(self.path, 'r') as f:\n",
    "            data = json.load(f)\n",
    "            self.__dict__.update(data)\n",
    "\n",
    "\n",
    "    @property\n",
    "    def dict(self):\n",
    "        return self.__dict__"
   ]
  }
